@article{barkov_discovery_1999,
  title = {Discovery of a narrow resonance state of the system {KSKSat} mass
           1520 {MeV}},
  volume = {70},
  issn = {1090-6487},
  url = {https://doi.org/10.1134/1.568160},
  doi = {10.1134/1.568160},
  abstract = {Data are presented which indicate the existence of a previously
              unknown narrow resonance near mass 1520 {MeV}, discovered while
              analyzing the system {KSKS}. The data were obtained at {ITEP} using
              the 6-m magnetic spectrometer in a beam of 40-{GeV} negative
              particles. The resonance is observed in π−p and K−p interactions
              with a total statistical confidence of not less than 5 standard
              deviations. The width of this state is comparable to the mass
              resolution of the spectrometer (∼5 {MeV}). The mass is 1521.5±2.5 {
              MeV}. Estimates of the product σ·{BR} ({KSKS}) give ∼5.0 nb in π−p
              interactions and ∼250 nb in K−p interactions. From the fact that
              the production cross section of this resonance in the K− beam is
              approximately 50 times greater than the production cross section in
              the π− beam, it can be concluded that this new state belongs to a
              system of mesons with hidden strangeness.},
