@article{oller_chiral_2001,
  title = {Chiral dynamics in the presence of bound states: kaon–nucleon
           interactions revisited},
  volume = {500},
  issn = {0370-2693},
  url = {https://www.sciencedirect.com/science/article/pii/S0370269301000788},
  doi = {10.1016/S0370-2693(01)00078-8},
  shorttitle = {Chiral dynamics in the presence of bound states},
  abstract = {We study the S-wave kaon–nucleon interactions for strangeness S=−1
              in a novel relativistic chiral unitary approach based on coupled
              channels. Dispersion relations are used to perform the necessary
              resummation of the lowest order relativistic chiral Lagrangian. A
              good description of the data in the K−p, πΣ and πΛ channels is
              obtained. We show how this method can be systematically extended to
              higher orders, emphasizing its applicability to any scenario of
              strong self-interactions where the perturbative series diverges
              even at low energies. Discussions about the differences to existing
              approaches employing pseudo-potentials in a regulated
              Lippmann–Schwinger equation are included. Finally, we describe the
              resonance content of our meson–baryon amplitudes and discuss its
              nature.},
  pages = {263--272},
  number = {3},
  journaltitle = {Physics Letters B},
  shortjournal = {Physics Letters B},
  author = {Oller, J. A. and Meißner, Ulf-G.},
  urldate = {2025-03-10},
  date = {2001-02-22},
  file = {ScienceDirect
          Snapshot:/home/dene/Zotero/storage/JFWV3D5V/S0370269301000788.html:text/html;Submitted
          Version:/home/dene/Zotero/storage/28X793GS/Oller and Meißner - 2001 -
          Chiral dynamics in the presence of bound states kaon–nucleon
          interactions revisited.pdf:application/pdf},
}
