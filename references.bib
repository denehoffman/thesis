@article{wilson_confinement_1974,
  title = {Confinement of quarks},
  volume = {10},
  url = {https://link.aps.org/doi/10.1103/PhysRevD.10.2445},
  doi = {10.1103/PhysRevD.10.2445},
  abstract = {A mechanism for total confinement of quarks, similar to that of
              Schwinger, is defined which requires the existence of Abelian or
              non-Abelian gauge fields. It is shown how to quantize a gauge field
              theory on a discrete lattice in Euclidean space-time, preserving
              exact gauge invariance and treating the gauge fields as angular
              variables (which makes a gauge-fixing term unnecessary). The
              lattice gauge theory has a computable strong-coupling limit; in
              this limit the binding mechanism applies and there are no free
              quarks. There is unfortunately no Lorentz (or Euclidean) invariance
              in the strong-coupling limit. The strong-coupling expansion
              involves sums over all quark paths and sums over all surfaces (on
              the lattice) joining quark paths. This structure is reminiscent of
              relativistic string models of hadrons.},
  pages = {2445--2459},
  number = {8},
  journaltitle = {Physical Review D},
  shortjournal = {Phys. Rev. D},
  author = {Wilson, Kenneth G.},
  urldate = {2024-06-12},
  date = {1974-10-15},
  note = {Publisher: American Physical Society},
}

@article{for_the_hadron_spectrum_collaboration_resonances_2015,
  title = {Resonances in coupled \${\textbackslash}ensuremath\{{\textbackslash}
           pi\}K,{\textbackslash}ensuremath\{{\textbackslash}eta\}K\$ scattering
           from lattice {QCD}},
  volume = {91},
  url = {https://link.aps.org/doi/10.1103/PhysRevD.91.054008},
  doi = {10.1103/PhysRevD.91.054008},
  abstract = {Coupled-channel 𝜋⁢𝐾 and 𝜂⁢𝐾 scattering amplitudes are determined
              by studying the finite-volume energy spectra obtained from
              dynamical lattice {QCD} calculations. Using a large basis of
              interpolating operators, including both those resembling a 𝑞⁢¯𝑞
              construction and those resembling a pair of mesons with relative
              momentum, a reliable excited-state spectrum can be obtained.
              Working at 𝑚𝜋=391 {MeV}, we find a gradual increase in the 𝐽𝑃=0+
              𝜋⁢𝐾 phase shift which may be identified with a broad scalar
              resonance that couples strongly to 𝜋⁢𝐾 and weakly to 𝜂⁢𝐾. The
              low-energy behavior of this amplitude suggests a virtual bound
              state that may be related to the 𝜅 resonance. A bound state with
              𝐽𝑃=1− is found very close to the 𝜋⁢𝐾 threshold energy, whose
              coupling to the 𝜋⁢𝐾 channel is compatible with that of the
              experimental 𝐾⋆⁡(892). Evidence is found for a narrow resonance in
              𝐽𝑃=2+. Isospin-3/2 𝜋⁢𝐾 scattering is also studied, and nonresonant
              phase shifts spanning the whole elastic scattering region are
              obtained.},
  pages = {054008},
  number = {5},
  journaltitle = {Physical Review D},
  shortjournal = {Phys. Rev. D},
  author = {{for the Hadron Spectrum Collaboration} and Wilson, David J. and
            Dudek, Jozef J. and Edwards, Robert G. and Thomas, Christopher E.},
  urldate = {2024-06-13},
  date = {2015-03-10},
  note = {Publisher: American Physical Society},
  file = {Accepted Version:/home/dene/Zotero/storage/4CGVBFSH/for the Hadron
          Spectrum Collaboration et al. - 2015 - Resonances in coupled \$
          ensuremath pi K,ensurema.pdf:application/pdf;APS
          Snapshot:/home/dene/Zotero/storage/H5YW2UNE/PhysRevD.91.html:text/html},
}

